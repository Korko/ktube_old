{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/korko/socialite"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "laravel/socialite": "dev-2.0-refreshToken as 2.0.x-dev",
        "socialiteproviders/dailymotion": "dev-master",
        "socialiteproviders/vimeo": "dev-master",
        "google/apiclient": "1.0.*@dev",
        "dailymotion/sdk": "dev-master",
        "vinkla/hashids": "^2.0@dev",
        "spatie/laravel-backup": "dev-master",
        "league/statsd": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6@dev",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "barryvdh/laravel-debugbar": "^2.0@dev"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Korko\\kTube\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
